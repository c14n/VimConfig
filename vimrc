" Set defaults that may be overridden by plugins.

set tabstop=8
set shiftwidth=8
set softtabstop=8
set noexpandtab
set autoindent

set backspace=indent,eol,start

set number

set list
set listchars=tab:→\ ,eol:↵

" Initialize Vundle and bundles.
source ~/.vim/vundlerc

